@-webkit-keyframes bouncefront {
        0% { -webkit-transform: translate3d(0,   0px,0); 
             -webkit-animation-timing-function: ease-out; }
       50% { -webkit-transform: translate3d(0,-90px,0); 
             -webkit-animation-timing-function: ease-in; }
      100% { -webkit-transform: translate3d(0,   0px,0); }
}
    

@-webkit-keyframes bouncebback {
        0% { -webkit-transform: translate3d(0,   0px,0); 
             -webkit-animation-timing-function: ease-out; }
       50% { -webkit-transform: translate3d(0,90px,0); 
             -webkit-animation-timing-function: ease-in; }
      100% { -webkit-transform: translate3d(0,   0px,0); }
}
    
    

@-webkit-keyframes movetoleft {
        0% { -webkit-transform: translate3d(420, 0px,0); 
             -webkit-animation-timing-function: ease-out; }     
      100% { -webkit-transform: translate3d(-410px, 0px,0);
             }
}

@-webkit-keyframes movetoleftout {
        0% { -webkit-transform: translate3d(420, 0px,0); 
             -webkit-animation-timing-function: ease-out; }     
      100% { -webkit-transform: translate3d(-250px, 0px,0);
             }
}
    
    
@-webkit-keyframes movetoright {
        0% { -webkit-transform: translate3d(10, 0px,0); 
             -webkit-animation-timing-function: ease-out; }     
      100% { -webkit-transform: translate3d(420px, 0px,0);}
}
@-webkit-keyframes movetorightout {
        0% { -webkit-transform: translate3d(10, 0px,0); 
             -webkit-animation-timing-function: ease-out; }     
      100% { -webkit-transform: translate3d(20px, 0px,0);}
}

